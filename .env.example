# Docsie Master Key for JWT Generation
# Get this from your Docsie deployment settings: https://app.docsie.io
DOCSIE_MASTER_KEY=your-secure-master-key-here

# Docsie Deployment Key (for secure portal)
# Example: deployment_No7ZEhXLoDHoW4RH7
DOCSIE_DEPLOYMENT_KEY=your_deployment_id_here

# Redirect URL for authentication fallback
# This is where users are redirected if JWT authentication fails
DOCSIE_REDIRECT_URL=http://localhost:5145/api/auth/login

# JWT Settings
JWT_EXPIRY_MINUTES=60
